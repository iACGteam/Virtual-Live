{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.array-buffer.detached.js\";\nimport \"core-js/modules/es.array-buffer.transfer.js\";\nimport \"core-js/modules/es.array-buffer.transfer-to-fixed-length.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.typed-array.with.js\";\nimport \"core-js/modules/esnext.json.parse.js\";\nimport \"core-js/modules/esnext.uint8-array.set-from-base64.js\";\nimport \"core-js/modules/esnext.uint8-array.set-from-hex.js\";\nimport \"core-js/modules/esnext.uint8-array.to-base64.js\";\nimport \"core-js/modules/esnext.uint8-array.to-hex.js\";\nimport \"core-js/modules/web.dom-exception.stack.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { ref, computed, onMounted, onBeforeUnmount } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nexport default {\n  __name: 'Video',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const route = useRoute();\n\n    // ËßÜÈ¢ëÊ∫êÂú∞ÂùÄ\n    const videoSrc = ref('https://www.w3schools.com/html/mov_bbb.mp4');\n    // ËßÜÈ¢ë‰ø°ÊÅØ\n    const videoInfo = ref(null);\n    // ÈáäÊîæ blob ËÆ∞ÂΩï\n    let blobUrl = null;\n    const isFollowing = ref(false);\n    const toggleFollow = () => {\n      isFollowing.value = !isFollowing.value;\n    };\n\n    // ======== Âä†ÂÖ•ÂúàÂ≠êÁä∂ÊÄÅ ========\n    const isJoined = ref(false);\n    const toggleJoin = () => {\n      isJoined.value = !isJoined.value;\n    };\n    const goToChannel = () => {\n      router.push({\n        path: '/profile'\n      });\n      // query: { id: videoInfo.value.creatorId }\n\n      console.log(\"Ë∑≥ËΩ¨Âà∞‰ΩúËÄÖ‰∏ªÈ°µÈÄªËæëËøôÈáåÂÜô\");\n    };\n\n    // ËØÑËÆ∫Êï∞ÊçÆÁ§∫‰æã\n    const comments = ref([{\n      id: 1,\n      user: 'Alice',\n      content: 'Â§™Ê£í‰∫ÜÔºÅ',\n      time: '2025-11-23T17:00:00',\n      hot: 10\n    }, {\n      id: 2,\n      user: 'Bob',\n      content: 'ÈùûÂ∏∏ÂñúÊ¨¢ÔºÅ',\n      time: '2025-11-23T17:05:00',\n      hot: 15\n    }, {\n      id: 3,\n      user: 'Charlie',\n      content: 'Â≠¶‰π†‰∫Ü',\n      time: '2025-11-23T17:10:00',\n      hot: 8\n    }]);\n\n    // ËØÑËÆ∫Â¢ûÂº∫\n    comments.value = comments.value.map(c => ({\n      ...c,\n      likes: c.hot || 0,\n      liked: false,\n      replies: []\n    }));\n\n    // ÈÄâÊã©ÊéíÂ∫èÊñπÂºè\n    const sortOrder = ref('time');\n    const sortedComments = computed(() => {\n      return [...comments.value].sort((a, b) => {\n        if (sortOrder.value === 'time') {\n          return new Date(b.time) - new Date(a.time);\n        } else if (sortOrder.value === 'hot') {\n          return b.hot - a.hot;\n        }\n        return 0;\n      });\n    });\n\n    // ÁÇπËµûÂäüËÉΩÔºàËßÜÈ¢ëÊàñËØÑËÆ∫ÈÄöÁî®Ôºâ\n    const toggleLike = item => {\n      if (!item.liked) {\n        item.likes++;\n        item.liked = true;\n      } else {\n        item.likes--;\n        item.liked = false;\n      }\n    };\n\n    // ÂèëÂ∏ÉËØÑËÆ∫\n    const newComment = ref(\"\");\n    const replyingTo = ref(null);\n    const replyText = ref(\"\");\n    const postComment = () => {\n      if (!newComment.value.trim()) return;\n      comments.value.unshift({\n        id: Date.now(),\n        user: \"You\",\n        content: newComment.value,\n        time: new Date().toISOString(),\n        likes: 0,\n        replies: []\n      });\n      newComment.value = \"\";\n    };\n\n    // ÂõûÂ§çÊ°ÜÂºÄÂÖ≥\n    const toggleReplyBox = comment => {\n      if (replyingTo.value === comment.id) {\n        replyingTo.value = null;\n      } else {\n        replyingTo.value = comment.id;\n        replyText.value = \"\";\n      }\n    };\n\n    // ÂõûÂ§çÊèê‰∫§\n    const submitReply = comment => {\n      if (!replyText.value.trim()) return;\n      comment.replies.push({\n        id: Date.now(),\n        user: \"You\",\n        content: replyText.value,\n        time: new Date().toISOString()\n      });\n      replyText.value = \"\";\n      replyingTo.value = null;\n    };\n\n    // ========= Êé®ËçêËßÜÈ¢ëÂÅáÊï∞ÊçÆ (ÂèØÊõøÊç¢ÁúüÂÆû API) =========\n\n    const recommendedVideos = ref([{\n      id: 101,\n      title: \"Ê†áÈ¢ò\",\n      author: \"‰ΩúËÄÖ\",\n      views: \"12‰∏á\",\n      thumbnail: \"https://picsum.photos/200/120?1\"\n    }, {\n      id: 102,\n      title: \"Ê†áÈ¢ò\",\n      author: \"‰ΩúËÄÖ\",\n      views: \"8.4‰∏á\",\n      thumbnail: \"https://picsum.photos/200/120?2\"\n    }, {\n      id: 103,\n      title: \"Ê†áÈ¢ò\",\n      author: \"‰ΩúËÄÖ\",\n      views: \"34‰∏á\",\n      thumbnail: \"https://picsum.photos/200/120?3\"\n    }, {\n      id: 104,\n      title: \"Ê†áÈ¢ò\",\n      author: \"‰ΩúËÄÖ\",\n      views: \"19‰∏á\",\n      thumbnail: \"https://picsum.photos/200/120?4\"\n    }, {\n      id: 105,\n      title: \"Ê†áÈ¢ò\",\n      author: \"‰ΩúËÄÖ\",\n      views: \"3.3‰∏á\",\n      thumbnail: \"https://picsum.photos/200/120?5\"\n    }]);\n\n    // ÁÇπÂáªÊé®ËçêËßÜÈ¢ë -> Ë∑≥ËΩ¨Êí≠Êîæ\n    const openRecommend = item => {\n      router.push({\n        path: \"/video\",\n        query: {\n          id: item.id,\n          src: videoSrc.value\n        }\n      });\n    };\n\n    // Ëá™Âä®Âä†ËΩΩËßÜÈ¢ë\n    const loadVideo = () => {\n      const videoSrc_query = route.query.src;\n      if (videoSrc_query) {\n        videoSrc.value = videoSrc_query;\n        const videoId = route.query.id;\n        if (videoId) {\n          videoInfo.value = {\n            id: videoId,\n            title: `ËßÜÈ¢ë ${videoId}`,\n            creator: 'VirtuaLive',\n            subs: '6666',\n            likes: '888',\n            views: 'ÁÇπÂáªÊí≠Êîæ',\n            duration: 'Âä®ÊÄÅÂä†ËΩΩ'\n          };\n        }\n        return;\n      }\n      const videoId = route.query.id;\n      if (videoId) {\n        try {\n          const userWorks = localStorage.getItem('userWorks');\n          if (userWorks) {\n            const works = JSON.parse(userWorks);\n            const video = works.find(w => w.id === Number(videoId));\n            if (video) {\n              videoInfo.value = video;\n              const blobUrl = sessionStorage.getItem(`videoBlob_${videoId}`);\n              if (blobUrl) {\n                videoSrc.value = blobUrl;\n                return;\n              }\n              if (video.videoData) {\n                if (video.videoData.startsWith('data:')) {\n                  videoSrc.value = video.videoData;\n                } else {\n                  const byteCharacters = atob(video.videoData);\n                  const byteNumbers = new Array(byteCharacters.length);\n                  for (let i = 0; i < byteCharacters.length; i++) {\n                    byteNumbers[i] = byteCharacters.charCodeAt(i);\n                  }\n                  const byteArray = new Uint8Array(byteNumbers);\n                  const blob = new Blob([byteArray], {\n                    type: video.fileType || 'video/mp4'\n                  });\n                  blobUrl = URL.createObjectURL(blob);\n                  videoSrc.value = blobUrl;\n                }\n                return;\n              }\n            }\n          }\n        } catch (err) {\n          console.error('Âä†ËΩΩËßÜÈ¢ëÂ§±Ë¥•:', err);\n        }\n      }\n      videoSrc.value = 'https://www.w3schools.com/html/mov_bbb.mp4';\n    };\n    onMounted(() => loadVideo());\n    onBeforeUnmount(() => {\n      if (blobUrl) {\n        URL.revokeObjectURL(blobUrl);\n        blobUrl = null;\n      }\n    });\n    const __returned__ = {\n      router,\n      route,\n      videoSrc,\n      videoInfo,\n      get blobUrl() {\n        return blobUrl;\n      },\n      set blobUrl(v) {\n        blobUrl = v;\n      },\n      isFollowing,\n      toggleFollow,\n      isJoined,\n      toggleJoin,\n      goToChannel,\n      comments,\n      sortOrder,\n      sortedComments,\n      toggleLike,\n      newComment,\n      replyingTo,\n      replyText,\n      postComment,\n      toggleReplyBox,\n      submitReply,\n      recommendedVideos,\n      openRecommend,\n      loadVideo,\n      ref,\n      computed,\n      onMounted,\n      onBeforeUnmount,\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","onBeforeUnmount","useRouter","useRoute","router","route","videoSrc","videoInfo","blobUrl","isFollowing","toggleFollow","value","isJoined","toggleJoin","goToChannel","push","path","console","log","comments","id","user","content","time","hot","map","c","likes","liked","replies","sortOrder","sortedComments","sort","a","b","Date","toggleLike","item","newComment","replyingTo","replyText","postComment","trim","unshift","now","toISOString","toggleReplyBox","comment","submitReply","recommendedVideos","title","author","views","thumbnail","openRecommend","query","src","loadVideo","videoSrc_query","videoId","creator","subs","duration","userWorks","localStorage","getItem","works","JSON","parse","video","find","w","Number","sessionStorage","videoData","startsWith","byteCharacters","atob","byteNumbers","Array","length","i","charCodeAt","byteArray","Uint8Array","blob","Blob","type","fileType","URL","createObjectURL","err","error","revokeObjectURL"],"sources":["C:/Users/13765/Desktop/vitualieve/front/Virtual-Live/front/src/components/video/Video.vue"],"sourcesContent":["<template>\r\n  <div class=\"video-page-container\">\r\n    <!-- Â∑¶‰æßÂÜÖÂÆπÂå∫ -->\r\n    <div class=\"left-content\">\r\n      <!-- ËßÜÈ¢ëÊí≠ÊîæÂå∫ÂüüÔºà‰øùÁïô‰Ω†ÁöÑÈÄªËæëÔºâ -->\r\n      <div class=\"video-wrapper\">\r\n        <video ref=\"videoRef\" :src=\"videoSrc\" controls autoplay></video>\r\n      </div>\r\n      <!-- ËßÜÈ¢ë‰ø°ÊÅØ -->\r\n      <div v-if=\"videoInfo\" class=\"video-meta\">\r\n        <h2 class=\"title\">{{ videoInfo.title }}</h2>\r\n        <div class=\"meta-row\">\r\n          <!-- UP ‰∏ª‰ø°ÊÅØ -->\r\n          <div class=\"creator-box\">\r\n            <div class=\"avatar\" @click=\"goToChannel\"></div>\r\n\r\n            <div class=\"channel-info\" @click=\"goToChannel\">\r\n              <p class=\"creator-name\">\r\n                {{ videoInfo.creator }}\r\n                <span class=\"verified\">‚úî</span>\r\n              </p>\r\n              <p class=\"sub-count\">{{ videoInfo.subs }} ËÆ¢ÈòÖ</p>\r\n            </div>\r\n\r\n            <div class=\"button-area\">\r\n              <button class=\"follow-btn\" @click=\"toggleFollow\">\r\n                {{ isFollowing ? 'Â∑≤ÂÖ≥Ê≥® ‚úì' : 'ÂÖ≥Ê≥®' }}\r\n              </button>\r\n\r\n              <button class=\"join-btn\" @click=\"toggleJoin\">\r\n                {{ isJoined ? 'Â∑≤Âä†ÂÖ• ‚úî' : 'Âä†ÂÖ•ÂúàÂ≠ê' }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- ‰∫íÂä®ÊåâÈíÆ -->\r\n          <div class=\"action-buttons\">\r\n            <button class=\"act-btn\" @click=\"toggleLike(videoInfo)\">\r\n              üëç {{ videoInfo.likes ?? 0 }}\r\n            </button>\r\n            <button class=\"act-btn\">üîó ÂàÜ‰∫´</button>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stats-row\">\r\n          <span>{{ videoInfo.views }}</span> ¬∑\r\n          <span>{{ videoInfo.duration }}</span>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ËØÑËÆ∫Âå∫ -->\r\n      <div class=\"comments-section\">\r\n\r\n        <div class=\"comment-input-box\">\r\n          <textarea v-model=\"newComment\" placeholder=\"ÂèëË°®‰Ω†ÁöÑÁúãÊ≥ï‚Ä¶\"></textarea>\r\n          <button @click=\"postComment\">ÂèëË°®ËØÑËÆ∫</button>\r\n        </div>\r\n\r\n        <div class=\"comments-header\">\r\n          <h3>ËØÑËÆ∫</h3>\r\n          <select v-model=\"sortOrder\">\r\n            <option value=\"time\">ÊåâÊó∂Èó¥</option>\r\n            <option value=\"hot\">ÊåâÁÉ≠Â∫¶</option>\r\n          </select>\r\n        </div>\r\n\r\n        <ul class=\"comments-list\">\r\n          <li v-for=\"comment in sortedComments\" :key=\"comment.id\">\r\n            <div class=\"comment-item\">\r\n              <strong>{{ comment.user }}:</strong> {{ comment.content }}\r\n              <div class=\"comment-actions\">\r\n                <span class=\"like-btn\" :class=\"{ liked: comment.liked }\" @click=\"toggleLike(comment)\">\r\n                  ‚ù§Ô∏è {{ comment.likes }}\r\n                </span>\r\n                <span @click=\"toggleReplyBox(comment)\">üí¨ ÂõûÂ§ç</span>\r\n              </div>\r\n              <div v-if=\"replyingTo === comment.id\" class=\"reply-box\">\r\n                <textarea v-model=\"replyText\" placeholder=\"ÂõûÂ§çÂÜÖÂÆπ‚Ä¶\"></textarea>\r\n                <button @click=\"submitReply(comment)\">ÂèëÈÄÅ</button>\r\n              </div>\r\n              <ul class=\"reply-list\" v-if=\"comment.replies.length > 0\">\r\n                <li v-for=\"reply in comment.replies\" :key=\"reply.id\" class=\"reply-item\">\r\n                  <strong>{{ reply.user }}:</strong> {{ reply.content }}\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Âè≥‰æßÊé®ËçêËßÜÈ¢ëÂå∫ -->\r\n    <aside class=\"right-sidebar\">\r\n      <h3>Êé®ËçêËßÜÈ¢ë</h3>\r\n\r\n      <div class=\"recommend-card\" v-for=\"item in recommendedVideos\" :key=\"item.id\" @click=\"openRecommend(item)\">\r\n        <img class=\"thumb\" :src=\"item.thumbnail\" />\r\n        <div class=\"info\">\r\n          <p class=\"title\">{{ item.title }}</p>\r\n          <small>@{{ item.author }} ¬∑ {{ item.views }} Ê¨°ËßÇÁúã</small>\r\n        </div>\r\n      </div>\r\n    </aside>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script setup>\r\nimport { ref, computed, onMounted, onBeforeUnmount } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\n\r\nconst router = useRouter()\r\nconst route = useRoute()\r\n\r\n// ËßÜÈ¢ëÊ∫êÂú∞ÂùÄ\r\nconst videoSrc = ref('https://www.w3schools.com/html/mov_bbb.mp4')\r\n// ËßÜÈ¢ë‰ø°ÊÅØ\r\nconst videoInfo = ref(null)\r\n// ÈáäÊîæ blob ËÆ∞ÂΩï\r\nlet blobUrl = null\r\n\r\nconst isFollowing = ref(false)\r\n\r\nconst toggleFollow = () => {\r\n  isFollowing.value = !isFollowing.value\r\n}\r\n\r\n// ======== Âä†ÂÖ•ÂúàÂ≠êÁä∂ÊÄÅ ========\r\nconst isJoined = ref(false)\r\n\r\nconst toggleJoin = () => {\r\n  isJoined.value = !isJoined.value\r\n}\r\n\r\nconst goToChannel = () => {\r\n  router.push({ path: '/profile'})\r\n  // query: { id: videoInfo.value.creatorId }\r\n\r\n  console.log(\"Ë∑≥ËΩ¨Âà∞‰ΩúËÄÖ‰∏ªÈ°µÈÄªËæëËøôÈáåÂÜô\")\r\n}\r\n\r\n// ËØÑËÆ∫Êï∞ÊçÆÁ§∫‰æã\r\nconst comments = ref([\r\n  { id: 1, user: 'Alice', content: 'Â§™Ê£í‰∫ÜÔºÅ', time: '2025-11-23T17:00:00', hot: 10 },\r\n  { id: 2, user: 'Bob', content: 'ÈùûÂ∏∏ÂñúÊ¨¢ÔºÅ', time: '2025-11-23T17:05:00', hot: 15 },\r\n  { id: 3, user: 'Charlie', content: 'Â≠¶‰π†‰∫Ü', time: '2025-11-23T17:10:00', hot: 8 }\r\n])\r\n\r\n// ËØÑËÆ∫Â¢ûÂº∫\r\ncomments.value = comments.value.map(c => ({\r\n  ...c,\r\n  likes: c.hot || 0,\r\n  liked: false,\r\n  replies: []\r\n}))\r\n\r\n// ÈÄâÊã©ÊéíÂ∫èÊñπÂºè\r\nconst sortOrder = ref('time')\r\n\r\nconst sortedComments = computed(() => {\r\n  return [...comments.value].sort((a, b) => {\r\n    if (sortOrder.value === 'time') {\r\n      return new Date(b.time) - new Date(a.time)\r\n    } else if (sortOrder.value === 'hot') {\r\n      return b.hot - a.hot\r\n    }\r\n    return 0\r\n  })\r\n})\r\n\r\n// ÁÇπËµûÂäüËÉΩÔºàËßÜÈ¢ëÊàñËØÑËÆ∫ÈÄöÁî®Ôºâ\r\nconst toggleLike = (item) => {\r\n  if (!item.liked) {\r\n    item.likes++\r\n    item.liked = true\r\n  } else {\r\n    item.likes--\r\n    item.liked = false\r\n  }\r\n}\r\n\r\n// ÂèëÂ∏ÉËØÑËÆ∫\r\nconst newComment = ref(\"\")\r\nconst replyingTo = ref(null)\r\nconst replyText = ref(\"\")\r\n\r\nconst postComment = () => {\r\n  if (!newComment.value.trim()) return\r\n\r\n  comments.value.unshift({\r\n    id: Date.now(),\r\n    user: \"You\",\r\n    content: newComment.value,\r\n    time: new Date().toISOString(),\r\n    likes: 0,\r\n    replies: []\r\n  })\r\n\r\n  newComment.value = \"\"\r\n}\r\n\r\n// ÂõûÂ§çÊ°ÜÂºÄÂÖ≥\r\nconst toggleReplyBox = (comment) => {\r\n  if (replyingTo.value === comment.id) {\r\n    replyingTo.value = null\r\n  } else {\r\n    replyingTo.value = comment.id\r\n    replyText.value = \"\"\r\n  }\r\n}\r\n\r\n// ÂõûÂ§çÊèê‰∫§\r\nconst submitReply = (comment) => {\r\n  if (!replyText.value.trim()) return\r\n\r\n  comment.replies.push({\r\n    id: Date.now(),\r\n    user: \"You\",\r\n    content: replyText.value,\r\n    time: new Date().toISOString()\r\n  })\r\n\r\n  replyText.value = \"\"\r\n  replyingTo.value = null\r\n}\r\n\r\n\r\n// ========= Êé®ËçêËßÜÈ¢ëÂÅáÊï∞ÊçÆ (ÂèØÊõøÊç¢ÁúüÂÆû API) =========\r\n\r\nconst recommendedVideos = ref([\r\n  {\r\n    id: 101,\r\n    title: \"Ê†áÈ¢ò\",\r\n    author: \"‰ΩúËÄÖ\",\r\n    views: \"12‰∏á\",\r\n    thumbnail: \"https://picsum.photos/200/120?1\"\r\n  },\r\n  {\r\n    id: 102,\r\n    title: \"Ê†áÈ¢ò\",\r\n    author: \"‰ΩúËÄÖ\",\r\n    views: \"8.4‰∏á\",\r\n    thumbnail: \"https://picsum.photos/200/120?2\"\r\n  },\r\n  {\r\n    id: 103,\r\n    title: \"Ê†áÈ¢ò\",\r\n    author: \"‰ΩúËÄÖ\",\r\n    views: \"34‰∏á\",\r\n    thumbnail: \"https://picsum.photos/200/120?3\"\r\n  },\r\n  {\r\n    id: 104,\r\n    title: \"Ê†áÈ¢ò\",\r\n    author: \"‰ΩúËÄÖ\",\r\n    views: \"19‰∏á\",\r\n    thumbnail: \"https://picsum.photos/200/120?4\"\r\n  },\r\n  {\r\n    id: 105,\r\n    title: \"Ê†áÈ¢ò\",\r\n    author: \"‰ΩúËÄÖ\",\r\n    views: \"3.3‰∏á\",\r\n    thumbnail: \"https://picsum.photos/200/120?5\"\r\n  }\r\n])\r\n\r\n// ÁÇπÂáªÊé®ËçêËßÜÈ¢ë -> Ë∑≥ËΩ¨Êí≠Êîæ\r\nconst openRecommend = (item) => {\r\n  router.push({\r\n    path: \"/video\",\r\n    query: { id: item.id, src: videoSrc.value }\r\n  })\r\n}\r\n\r\n\r\n// Ëá™Âä®Âä†ËΩΩËßÜÈ¢ë\r\nconst loadVideo = () => {\r\n  const videoSrc_query = route.query.src\r\n  if (videoSrc_query) {\r\n    videoSrc.value = videoSrc_query\r\n    const videoId = route.query.id\r\n    if (videoId) {\r\n      videoInfo.value = {\r\n        id: videoId,\r\n        title: `ËßÜÈ¢ë ${videoId}`,\r\n        creator: 'VirtuaLive',\r\n        subs: '6666',\r\n        likes: '888',\r\n        views: 'ÁÇπÂáªÊí≠Êîæ',\r\n        duration: 'Âä®ÊÄÅÂä†ËΩΩ'\r\n      }\r\n    }\r\n    return\r\n  }\r\n\r\n  const videoId = route.query.id\r\n  if (videoId) {\r\n    try {\r\n      const userWorks = localStorage.getItem('userWorks')\r\n      if (userWorks) {\r\n        const works = JSON.parse(userWorks)\r\n        const video = works.find(w => w.id === Number(videoId))\r\n\r\n        if (video) {\r\n          videoInfo.value = video\r\n\r\n          const blobUrl = sessionStorage.getItem(`videoBlob_${videoId}`)\r\n          if (blobUrl) {\r\n            videoSrc.value = blobUrl\r\n            return\r\n          }\r\n\r\n          if (video.videoData) {\r\n            if (video.videoData.startsWith('data:')) {\r\n              videoSrc.value = video.videoData\r\n            } else {\r\n              const byteCharacters = atob(video.videoData)\r\n              const byteNumbers = new Array(byteCharacters.length)\r\n              for (let i = 0; i < byteCharacters.length; i++) {\r\n                byteNumbers[i] = byteCharacters.charCodeAt(i)\r\n              }\r\n              const byteArray = new Uint8Array(byteNumbers)\r\n              const blob = new Blob([byteArray], { type: video.fileType || 'video/mp4' })\r\n              blobUrl = URL.createObjectURL(blob)\r\n              videoSrc.value = blobUrl\r\n            }\r\n            return\r\n          }\r\n        }\r\n      }\r\n    } catch (err) {\r\n      console.error('Âä†ËΩΩËßÜÈ¢ëÂ§±Ë¥•:', err)\r\n    }\r\n  }\r\n\r\n  videoSrc.value = 'https://www.w3schools.com/html/mov_bbb.mp4'\r\n}\r\n\r\nonMounted(() => loadVideo())\r\n\r\nonBeforeUnmount(() => {\r\n  if (blobUrl) {\r\n    URL.revokeObjectURL(blobUrl)\r\n    blobUrl = null\r\n  }\r\n})\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* È°µÈù¢Êï¥‰ΩìÂ∏ÉÂ±Ä */\r\n.video-page-container {\r\n  display: flex;\r\n  gap: 20px;\r\n  padding: 20px;\r\n  min-height: 100vh;\r\n  background: #fefbff;\r\n}\r\n\r\n/* Â∑¶‰æßÂÜÖÂÆπÂ∏ÉÂ±Ä */\r\n.left-content {\r\n  flex: 2.2;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n/* ËßÜÈ¢ëÂå∫Âüü */\r\n.video-wrapper {\r\n  width: 100%;\r\n  aspect-ratio: 16 / 9;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  margin-bottom: 10px;\r\n}\r\n\r\nvideo {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 8px;\r\n  background-color: #000;\r\n  object-fit: contain;\r\n}\r\n\r\n/* ËßÜÈ¢ë‰ø°ÊÅØÊ®°Âùó */\r\n.video-meta {\r\n  background: #fefbff;\r\n  padding: 10px 15px;\r\n  border-bottom: 1px solid #ddd;\r\n  border-radius: 12px;\r\n  border: 1px solid #fedef0;\r\n  box-shadow: 0 4px 10px rgba(255, 105, 180, 0.15);\r\n}\r\n\r\n.video-meta .title {\r\n  margin: 0;\r\n  font-size: 20px;\r\n  font-weight: 600;\r\n  color: #2d2d2d;\r\n}\r\n\r\n.meta-row {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-top: 8px;\r\n}\r\n\r\n.creator-box {\r\n  display: flex;\r\n  align-items: center;\r\n  /* justify-content:left; */\r\n  width: 100%;\r\n  gap: 12px;\r\n  margin-top: 8px;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Â§¥ÂÉè */\r\n.avatar {\r\n  width: 44px;\r\n  height: 44px;\r\n  border-radius: 50%;\r\n  background: linear-gradient(135deg, #ddd, #bbb);\r\n  flex-shrink: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n/* È¢ëÈÅì‰ø°ÊÅØ */\r\n.channel-info {\r\n  /* flex: 1; */\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n/* È¢ëÈÅìÂêç */\r\n.creator-name {\r\n  font-size: 16px;\r\n  font-weight: 60