{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nexport default {\n  data() {\n    return {\n      followed: false,\n      showPostBox: false,\n      newPost: {\n        title: \"\",\n        content: \"\",\n        images: []\n      },\n      com: {\n        id: 1,\n        name: \"ÂÆòÊñπÁ§æÂõ¢\",\n        count: 23000,\n        days: 5\n      },\n      cards: [{\n        id: 1,\n        title: \"ÂìàÂìàÂìà\",\n        excerpt: 'ÂèàÊòØËß£Á¶ÅÁöÑ‰∏ÄÂº†ü•πÊòØÊî∂ËóèÈõÜÁöÑÈòøËê®AzaÔºå‰πüÊòØÂ∏¶Ëñ™Áîª‰∏ä‰∫Ü ‚Äã„ÄÇ',\n        avatar: require('@/assets/community/aza2.jpg'),\n        likes: 0,\n        liked: false,\n        showComments: false,\n        newComment: \"\",\n        replyTo: null,\n        highlight: false,\n        comments: [{\n          user: \"Áî®Êà∑A\",\n          text: \"ÂÜôÂæó‰∏çÈîôÔºÅ\",\n          avatar: require('@/assets/avatar.jpg'),\n          time: \"2Â∞èÊó∂Ââç\"\n        }, {\n          user: \"Áî®Êà∑B\",\n          text: \"ÂæàÊúâÂ∏ÆÂä©ÔºÅ\",\n          avatar: require('@/assets/avatar.jpg'),\n          time: \"1Â§©Ââç\"\n        }],\n        images: [require('@/assets/community/aza2.jpg')]\n      }, {\n        id: 2,\n        title: \"Ê†áÈ¢ò2\",\n        excerpt: 'Á¨¨‰∫åÊù°ÂÜÖÂÆπÁöÑÁÆÄÁü≠ÊèèËø∞ÔºåÈÖçÂêàÂç°ÁâáÂ∏ÉÂ±ÄÁúãËµ∑Êù•Êõ¥Êï¥ÈΩê„ÄÇ',\n        avatar: require('@/assets/avatar.jpg'),\n        likes: 3,\n        liked: true,\n        showComments: false,\n        newComment: \"\",\n        replyTo: null,\n        highlight: false,\n        comments: [],\n        images: []\n      }],\n      joined: false,\n      signed: false,\n      toast: {\n        show: false,\n        msg: ''\n      },\n      navList: ['ÊúÄÊñ∞', 'ÊúÄÁÉ≠'],\n      activeTab: 'ÊúÄÊñ∞'\n    };\n  },\n  methods: {\n    toggleJoin() {\n      this.joined = !this.joined;\n      if (this.joined && !this.signed) {\n        // Optionally keep signed false until user explicitly signs\n      }\n    },\n    handleSign() {\n      if (!this.joined) {\n        // Ëá™Âä®Âä†ÂÖ•Âπ∂Á≠æÂà∞\n        this.joined = true;\n      }\n      if (this.signed) {\n        this.showToast('‰Ω†‰ªäÂ§©Â∑≤ÁªèÁ≠æÂà∞Ëøá‰∫Ü');\n        return;\n      }\n\n      // ÊâßË°åÁ≠æÂà∞ÈÄªËæë\n      this.signed = true;\n      this.com.days += 1;\n      // ÁªôÁ¨¨‰∏Ä‰∏™Âç°Áâá‰∏Ä‰∏™ÁÆÄÁü≠È´ò‰∫ÆÂèçÈ¶à\n      this.cards[0].highlight = true;\n      setTimeout(() => this.cards[0].highlight = false, 800);\n      this.showToast('Á≠æÂà∞ÊàêÂäüÔºÅËøûÁª≠Á≠æÂà∞+' + 1);\n    },\n    showToast(msg = '', ms = 1200) {\n      this.toast.msg = msg;\n      this.toast.show = true;\n      setTimeout(() => this.toast.show = false, ms);\n    },\n    toggleComments(id) {\n      this.cards = this.cards.map(card => card.id === id ? {\n        ...card,\n        showComments: !card.showComments\n      } : card);\n    },\n    toggleLike(card) {\n      // Â∞èÂä®ÁîªÔºöÂÖàÂàáÊç¢ likedÔºåÂÜçË∞ÉÊï¥Êï∞Â≠ó\n      card.liked = !card.liked;\n      card.likes += card.liked ? 1 : -1;\n      // Ëß¶Âèë highlight Âä®Áîª\n      card.highlight = true;\n      setTimeout(() => card.highlight = false, 400);\n    },\n    startReply(cardId, user) {\n      const card = this.cards.find(c => c.id === cardId);\n      card.replyTo = user; // null Ë°®Á§∫‰∏çÊòØÂõûÂ§ç\n      card.showComments = true; // Ëá™Âä®Â±ïÂºÄËØÑËÆ∫Âå∫\n      // Â∞ÜÁÑ¶ÁÇπÊîæÂà∞ËæìÂÖ•Ê°ÜÔºà‰∏ã‰∏ÄÊ¨°ÂèØ‰ª•Áî® $nextTick Ëé∑ÂèñÂÖÉÁ¥†Âπ∂ focusÔºâ\n    },\n    submitComment(cardId) {\n      const card = this.cards.find(c => c.id === cardId);\n      if (!card.newComment || !card.newComment.trim()) return;\n      const text = card.replyTo ? `ÂõûÂ§ç ${card.replyTo}Ôºö${card.newComment}` : card.newComment;\n      card.comments.push({\n        user: \"Êàë\",\n        text,\n        avatar: require('@/assets/avatar.jpg'),\n        time: 'ÂàöÂàö'\n      });\n\n      // Ê∏ÖÁ©∫ËæìÂÖ•\n      card.newComment = \"\";\n      card.replyTo = null;\n      this.showToast('ËØÑËÆ∫Â∑≤ÂèëÂ∏É', 900);\n    },\n    goBack() {\n      if (window.history.length > 1) {\n        this.$router.go(-1);\n      } else {\n        this.$router.push({\n          path: '/'\n        });\n      }\n    },\n    toggleFollow() {\n      this.followed = !this.followed;\n      // this.showToast(this.followed ? \"Â∑≤ÂÖ≥Ê≥®ËØ•Âçö‰∏ª\" : \"Â∑≤ÂèñÊ∂àÂÖ≥Ê≥®\");\n    },\n    publishPost() {\n      if (!this.newPost.title.trim()) {\n        this.showToast(\"Ê†áÈ¢ò‰∏çËÉΩ‰∏∫Á©∫\");\n        return;\n      }\n      const newCard = {\n        id: Date.now(),\n        title: this.newPost.title,\n        excerpt: this.newPost.content.substring(0, 30),\n        avatar: require('@/assets/avatar.jpg'),\n        likes: 0,\n        liked: false,\n        showComments: false,\n        newComment: \"\",\n        replyTo: null,\n        highlight: true,\n        comments: [],\n        images: [...this.newPost.images]\n      };\n      this.cards.unshift(newCard);\n\n      // Ê∏ÖÁ©∫ËæìÂÖ•Ê°Ü\n      this.newPost.title = \"\";\n      this.newPost.content = \"\";\n      this.newPost.images = [];\n      this.showPostBox = false;\n      this.showToast(\"ÂèëÂ∏ÉÊàêÂäü\");\n      setTimeout(() => newCard.highlight = false, 800);\n    },\n    handleImageUpload(e) {\n      const files = Array.from(e.target.files);\n      if (this.newPost.images.length + files.length > 2) {\n        this.showToast(\"ÊúÄÂ§öÂè™ËÉΩÈÄâÊã© 2 Âº†ÂõæÁâá\");\n        return;\n      }\n      files.forEach(file => {\n        const reader = new FileReader();\n        reader.onload = event => {\n          this.newPost.images.push(event.target.result);\n        };\n        reader.readAsDataURL(file);\n      });\n\n      // Ê∏ÖÁ©∫ inputÔºåÂê¶ÂàôÊó†Ê≥ïÈáçÂ§çÈÄâÊã©Âêå‰∏ÄÂº†ÂõæÁâá\n      e.target.value = null;\n    },\n    removeImage(index) {\n      this.newPost.images.splice(index, 1);\n    }\n  },\n  computed: {\n    filteredCards() {\n      // ËøîÂõûÊñ∞ÁöÑÊï∞ÁªÑÂâØÊú¨ÔºåÈÅøÂÖç sort ÊîπÂèòÂéüÊï∞ÁªÑÈ°∫Â∫è\n      const list = this.cards.slice();\n      if (this.activeTab === 'ÊúÄÊñ∞') {\n        // ÂÅáËÆæÂç°ÁâáÊ≤°ÊúâÊó∂Èó¥Â≠óÊÆµÔºåÁî® id ÂÄíÂ∫è‰ª£Ë°®ÊúÄÊñ∞\n        return list.sort((a, b) => b.id - a.id);\n      }\n      if (this.activeTab === 'ÊúÄÁÉ≠') {\n        return list.sort((a, b) => b.likes - a.likes);\n      }\n      return list;\n    }\n  }\n};","map":{"version":3,"names":["data","followed","showPostBox","newPost","title","content","images","com","id","name","count","days","cards","excerpt","avatar","require","likes","liked","showComments","newComment","replyTo","highlight","comments","user","text","time","joined","signed","toast","show","msg","navList","activeTab","methods","toggleJoin","handleSign","showToast","setTimeout","ms","toggleComments","map","card","toggleLike","startReply","cardId","find","c","submitComment","trim","push","goBack","window","history","length","$router","go","path","toggleFollow","publishPost","newCard","Date","now","substring","unshift","handleImageUpload","e","files","Array","from","target","forEach","file","reader","FileReader","onload","event","result","readAsDataURL","value","removeImage","index","splice","computed","filteredCards","list","slice","sort","a","b"],"sources":["C:\\Users\\13765\\Desktop\\vitualieve\\front\\Virtual-Live\\front\\src\\components\\community\\ComDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"circle-detail\">\r\n    <div class=\"header-right\">\r\n      <button class=\"back-btn\" @click=\"goBack\">\r\n        <span class=\"back-icon\">‚Üê</span>\r\n        <span>ËøîÂõû</span>\r\n      </button>\r\n    </div>\r\n\r\n    <header>\r\n      <div class=\"touxiang\">\r\n        <img src=\"@/assets/avatar.jpg\" class=\"avatar\" />\r\n      </div>\r\n      <div class=\"info\">\r\n        <h2 class=\"name\">\r\n          {{ com.name }}\r\n          <span class=\"follow-btn\" :class=\"{ followed }\" @click=\"toggleFollow\">\r\n            {{ followed ? 'Â∑≤ÂÖ≥Ê≥® ‚úì' : 'ÂÖ≥Ê≥®' }}\r\n          </span>\r\n        </h2>\r\n\r\n        <div class=\"number\">\r\n          <p class=\"count\">{{ com.count }} Á≤â‰∏ù</p>\r\n          <p class=\"count\">{{ com.count }} ‰ΩúÂìÅÈáè</p>\r\n          <p class=\"count\">Â∑≤ÁªèËøûÁª≠Á≠æÂà∞{{ com.days }}Â§©</p>\r\n        </div>\r\n\r\n        <p class=\"intro\">‰ªãÁªç</p>\r\n\r\n      </div>\r\n\r\n      <div class=\"actions\">\r\n        <el-button class=\"join-btn\" :type=\"joined ? 'success' : ''\" @click=\"toggleJoin\">\r\n          <span v-if=\"joined\">Â∑≤Âä†ÂÖ• ‚úì</span>\r\n          <span v-else>Âä†ÂÖ•</span>\r\n        </el-button>\r\n\r\n        <el-button class=\"sign-btn\" :class=\"{ signed: signed }\r\n          \" :disabled=\"!joined || signed\" @click=\"handleSign\">\r\n          <template v-if=\"!joined\">Âä†ÂÖ•Âπ∂Á≠æÂà∞</template>\r\n          <template v-else-if=\"!signed\">ÁÇπÂáªÁ≠æÂà∞</template>\r\n          <template v-else>‰ªäÊó•Â∑≤Á≠æÂà∞ ‚úî</template>\r\n        </el-button>\r\n\r\n      </div>\r\n    </header>\r\n\r\n    <transition name=\"fade-slide\">\r\n      <div v-if=\"toast.show\" class=\"toast\">{{ toast.msg }}</div>\r\n    </transition>\r\n\r\n    <div class=\"nav-tabs\">\r\n      <div v-for=\"item in navList\" :key=\"item\" :class=\"['tab-item', activeTab === item ? 'active' : '']\"\r\n        @click=\"activeTab = item\">\r\n        {{ item }}\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"content-list\">\r\n      <div v-for=\"card in filteredCards\" :key=\"card.id\" class=\"card\" :class=\"{ highlight: card.highlight }\">\r\n\r\n        <!-- Âç°ÁâáÂÜÖÂÆπ -->\r\n        <div class=\"card-header\">\r\n          <div class=\"card-left\">\r\n            <p class=\"title\">{{ card.title }}</p>\r\n            <p class=\"excerpt\">{{ card.excerpt }}</p>\r\n            <div class=\"card-images\" v-if=\"card.images && card.images.length\">\r\n              <img v-for=\"(img, index) in card.images\" :key=\"index\" :src=\"img\" class=\"post-image\" />\r\n            </div>\r\n\r\n          </div>\r\n          <img :src=\"card.avatar\" class=\"card-avatar\" />\r\n        </div>\r\n\r\n        <!-- Â∫ïÈÉ®ÊåâÈíÆ -->\r\n        <footer class=\"card-footer\">\r\n          <div class=\"comments\" @click=\"toggleComments(card.id)\">\r\n            üí¨ Êü•Áúã {{ card.comments.length }} Êù°ËØÑËÆ∫\r\n          </div>\r\n          <div class=\"reply\" @click=\"startReply(card.id, null)\">‚Ü© ÂõûÂ§ç</div>\r\n          <div class=\"like\" @click=\"toggleLike(card)\">\r\n            <span :class=\"['heart', card.liked ? 'liked' : '']\">‚ù§</span>\r\n            <span class=\"likes-num\">{{ card.likes }}</span>\r\n          </div>\r\n        </footer>\r\n\r\n        <!-- ËØÑËÆ∫Âå∫ÔºàÂèØÂ±ïÂºÄÔºâ -->\r\n        <transition name=\"expand\">\r\n          <div class=\"comment-box\" v-if=\"card.showComments\">\r\n            <!-- ÂèëÂ∏ÉËØÑËÆ∫ -->\r\n            <div class=\"comment-input\">\r\n              <input type=\"text\" v-model=\"card.newComment\"\r\n                :placeholder=\"card.replyTo ? 'ÂõûÂ§çÔºö' + card.replyTo : 'ÂèëË°®‰Ω†ÁöÑËØÑËÆ∫...'\"\r\n                @keyup.enter=\"submitComment(card.id)\" />\r\n              <button @click=\"submitComment(card.id)\">ÂèëË°®</button>\r\n            </div>\r\n\r\n\r\n            <div class=\"comments-list\">\r\n              <div class=\"comment-item\" v-for=\"(c, index) in card.comments\" :key=\"index\">\r\n\r\n                <img :src=\"c.avatar\" class=\"comment-avatar\" />\r\n\r\n                <div class=\"comment-main\">\r\n                  <div class=\"comment-user\">{{ c.user }}</div>\r\n                  <div class=\"comment-content\">{{ c.text }}</div>\r\n\r\n                  <div class=\"comment-actions\">\r\n                    <span class=\"comment-time\">{{ c.time }}</span>\r\n                    <span class=\"reply-btn\" @click=\"startReply(card.id, c.user)\">ÂõûÂ§ç</span>\r\n                  </div>\r\n                </div>\r\n\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </transition>\r\n\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Âõ∫ÂÆöÂèëÂ∏ÉÊåâÈíÆ -->\r\n  <button class=\"post-btn\" @click=\"showPostBox = true\">\r\n    Ôºã\r\n  </button>\r\n\r\n  <!-- ÂèëÂ∏ÉÊ≥°Ê≥°Ê°Ü -->\r\n  <transition name=\"fade-slide\">\r\n    <div v-if=\"showPostBox\" class=\"post-bubble\">\r\n      <h3 class=\"post-title\">ÂèëÂ∏ÉÊñ∞Â∏ñÂ≠ê</h3>\r\n\r\n      <input v-model=\"newPost.title\" placeholder=\"ËæìÂÖ•Ê†áÈ¢ò\" class=\"post-input\" />\r\n\r\n      <textarea v-model=\"newPost.content\" placeholder=\"ÂÜôÁÇπ‰ªÄ‰πàÂêß...\" class=\"post-textarea\"></textarea>\r\n\r\n      <!-- ÂõæÁâá‰∏ä‰º† -->\r\n      <div class=\"post-upload\">\r\n        <label class=\"upload-btn\">\r\n          üì∑ ‰∏ä‰º†ÂõæÁâá\r\n          <input type=\"file\" accept=\"image/*\" multiple @change=\"handleImageUpload\" />\r\n        </label>\r\n\r\n        <div class=\"preview-box\">\r\n          <img v-for=\"(img, index) in newPost.images\" :key=\"index\" :src=\"img\" class=\"preview-img\"\r\n            @click=\"removeImage(index)\" />\r\n        </div>\r\n      </div>\r\n\r\n\r\n      <div class=\"post-actions\">\r\n        <button class=\"cancel\" @click=\"showPostBox = false\">ÂèñÊ∂à</button>\r\n        <button class=\"publish\" @click=\"publishPost\">ÂèëÂ∏É</button>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      followed: false,\r\n      showPostBox: false,\r\n      newPost: {\r\n        title: \"\",\r\n        content: \"\",\r\n        images: []\r\n      },\r\n      com: {\r\n        id: 1,\r\n        name: \"ÂÆòÊñπÁ§æÂõ¢\",\r\n        count: 23000,\r\n        days: 5\r\n      },\r\n      cards: [\r\n        {\r\n          id: 1,\r\n          title: \"ÂìàÂìàÂìà\",\r\n          excerpt: 'ÂèàÊòØËß£Á¶ÅÁöÑ‰∏ÄÂº†ü•πÊòØÊî∂ËóèÈõÜÁöÑÈòøËê®AzaÔºå‰πüÊòØÂ∏¶Ëñ™Áîª‰∏ä‰∫Ü ‚Äã„ÄÇ',\r\n          avat