eviewSCStyle","_hoisted_26","_hoisted_27","currentUser","_hoisted_28","input","_hoisted_29","_hoisted_30","confirmSendSC","disabled","trim","_hoisted_31","_hoisted_32","for","toggleSCForm","_hoisted_33","onKeydown","_withKeys","_withModifiers","onSend","_hoisted_35"],"sources":["C:\\Users\\13765\\Desktop\\vitualieve\\front\\Virtual-Live\\front\\src\\components\\liveroom\\LiveChat.vue"],"sourcesContent":["<template>\r\n  <div class=\"live-chat\" ref=\"chatContainer\" @mousemove=\"onMouseMove\">\r\n    <!-- È°∂ÈÉ®ÊÇ¨ÊµÆËß¶ÂèëÂå∫‰∏éÊéíË°åÊ¶ú -->\r\n    <div class=\"chat-top\" ref=\"chatTop\">\r\n      <div class=\"room-title\">\r\n        ËÅäÂ§©Âå∫\r\n        <span class=\"online\">Âú®Á∫ø {{ fakeOnline }}</span>\r\n      </div>\r\n\r\n      <!-- Á§ºÁâ©Ê¶ú -->\r\n      <div\r\n        class=\"gift-leaderboard\"\r\n        v-show=\"showLeaderboard\"\r\n        @mouseenter=\"keepLeaderboard = true\"\r\n        @mouseleave=\"keepLeaderboard = false\"\r\n      >\r\n        <div class=\"lb-title\">ÈÄÅÁ§ºÊéíË°åÊ¶ú</div>\r\n        <ol>\r\n          <li v-for=\"(u, idx) in leaderboard\" :key=\"u.username\">\r\n            <span class=\"rank\">{{ idx + 1 }}</span>\r\n            <span class=\"name\">{{ u.username }}</span>\r\n            <span class=\"amount\">{{ u.amount }} Á§ºÁâ©ÂÄº</span>\r\n          </li>\r\n        </ol>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Âõ∫ÂÆöÊòæÁ§∫ÁöÑ SC ÂºπÂπïÂå∫ÂüüÔºàÊåÅ‰πÖ 10 ÁßíÔºâ -->\r\n    <div class=\"sc-pinned\" v-if=\"scPinned.length > 0\">\r\n      <div\r\n        class=\"sc-item\"\r\n        v-for=\"p in scPinned\"\r\n        :key=\"p.id\"\r\n        :style=\"bubbleStyle(p)\"\r\n      >\r\n        <div class=\"meta\">\r\n          <span class=\"username\">{{ p.username }}</span>\r\n          <span class=\"sc-badge\">SC</span>\r\n        </div>\r\n        <div class=\"content\">{{ p.content }}</div>\r\n        <div class=\"sc-amount\">¬•{{ p.scAmount }}</div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Ê∂àÊÅØÂàóË°® -->\r\n    <div class=\"messages\" ref=\"messagesList\">\r\n      <div\r\n        v-for=\"msg in messages\"\r\n        :key=\"msg.id\"\r\n        class=\"msg\"\r\n        :class=\"msg.type\"\r\n      >\r\n        <div class=\"avatar\" :style=\"{ background: avatarColor(msg.username) }\">\r\n          {{ avatarInitial(msg.username) }}\r\n        </div>\r\n\r\n        <div class=\"bubble\" :style=\"bubbleStyle(msg)\">\r\n          <div class=\"meta\">\r\n            <span class=\"username\">{{ msg.username }}</span>\r\n\r\n            <span v-if=\"msg.type === 'sc'\" class=\"sc-badge\">SC</span>\r\n            <span v-if=\"msg.type === 'gift'\" class=\"gift-badge\">üéÅ</span>\r\n          </div>\r\n\r\n          <div class=\"content\">{{ msg.content }}</div>\r\n\r\n          <div v-if=\"msg.type === 'sc' && msg.scAmount\" class=\"sc-amount\">\r\n            ¬•{{ msg.scAmount }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- SC ÂèëÈÄÅË°®Âçï -->\r\n    <div class=\"sc-form\" v-if=\"showSCForm\">\r\n      <div class=\"sc-form-header\">\r\n        <span>ÂèëÈÄÅ SC ÂºπÂπï</span>\r\n        <button class=\"close-btn\" @click=\"closeSCForm\">√ó</button>\r\n      </div>\r\n      \r\n      <div class=\"sc-amount-selector\">\r\n        <div class=\"amount-title\">ÈÄâÊã©ÈáëÈ¢ùÔºàÂÖÉÔºâ</div>\r\n        <div class=\"amount-options\">\r\n          <button\r\n            v-for=\"amount in scAmountOptions\"\r\n            :key=\"amount\"\r\n            class=\"amount-btn\"\r\n            :class=\"{ active: selectedSCAmount === amount }\"\r\n            @click=\"selectSCAmount(amount)\"\r\n          >\r\n            ¬•{{ amount }}\r\n          </button>\r\n          <div class=\"custom-amount\">\r\n            <span>Ëá™ÂÆö‰πâÔºö</span>\r\n            <input\r\n              type=\"number\"\r\n              v-model=\"customAmount\"\r\n              min=\"30\"\r\n              max=\"9999\"\r\n              placeholder=\"ËæìÂÖ•ÈáëÈ¢ù\"\r\n              @input=\"onCustomAmountInput\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"sc-preview\">\r\n        <div class=\"preview-title\">È¢ÑËßàÊïàÊûú</div>\r\n        <div class=\"sc-preview-bubble\" :style=\"previewSCStyle\">\r\n          <div class=\"meta\">\r\n            <span class=\"username\">{{ currentUser.username }}</span>\r\n            <span class=\"sc-badge\">SC</span>\r\n          </div>\r\n          <div class=\"content\">{{ input || 'ËØ∑ËæìÂÖ•ÂºπÂπïÂÜÖÂÆπ' }}</div>\r\n          <div class=\"sc-amount\">¬•{{ selectedSCAmount }}</div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"sc-form-actions\">\r\n        <button class=\"cancel-btn\" @click=\"closeSCForm\">ÂèñÊ∂à</button>\r\n        <button class=\"confirm-btn\" @click=\"confirmSendSC\" :disabled=\"!input.trim()\">\r\n          Á°ÆËÆ§ÂèëÈÄÅÔºà¬•{{ selectedSCAmount }}Ôºâ\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- SC ÂãæÈÄâÂºÄÂÖ≥ -->\r\n    <div class=\"sc-toggle\" v-if=\"!showSCForm\">\r\n      <!-- <input type=\"checkbox\" v-model=\"sendAsSC\" id=\"scCheck\" /> -->\r\n      <label for=\"scCheck\" @click=\"toggleSCForm\">Ëä±Ë¥π{{ selectedSCAmount }}ÂÖÉÂèëÈÄÅSCÂºπÂπï</label>\r\n    </div>\r\n\r\n    <!-- ÂèëÈÄÅÂå∫ -->\r\n    <div class=\"send-box\">\r\n      <input\r\n        v-model=\"input\"\r\n        @keydown.enter.prevent=\"onSend\"\r\n        placeholder=\"Êåâ Enter ÂèëÈÄÅ\"\r\n        :disabled=\"showSCForm\"\r\n      />\r\n      <button @click=\"onSend\" :disabled=\"showSCForm\">ÂèëÈÄÅ</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"LiveChat\",\r\n  props: {\r\n    messages: Array,\r\n    currentUser: Object,\r\n  },\r\n  emits: [\"send-message\", \"send-sc\"],\r\n  data() {\r\n    return {\r\n      input: \"\",\r\n      sendAsSC: false,\r\n      showSCForm: false, // Êñ∞Â¢ûÔºöÊéßÂà∂ SC Ë°®ÂçïÊòæÁ§∫\r\n      scAmountOptions: [30, 50, 100, 200, 500, 1000],\r\n      selectedSCAmount: 50,\r\n      customAmount: \"\",\r\n      scPinned: [],\r\n      fakeOnline: 8523,\r\n\r\n      showLeaderboard: false,\r\n      keepLeaderboard: false,\r\n      leaderboard: [\r\n        { username: \"ÂúüË±™B\", amount: 5200 },\r\n        { username: \"Â§ß‰Ω¨SC\", amount: 3000 },\r\n        { username: \"Â∞èËêåÊñ∞\", amount: 900 },\r\n      ],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    previewSCStyle() {\r\n      return {\r\n        background: \"linear-gradient(90deg, rgba(255,123,123,0.12), rgba(255,187,123,0.08))\",\r\n        border: \"1px solid rgba(255,123,123,0.25)\",\r\n      };\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.scrollToBottom();\r\n  },\r\n  watch: {\r\n    messages() {\r\n      this.$nextTick(() => this.scrollToBottom());\r\n    },\r\n    keepLeaderboard(v) {\r\n      if (v) this.showLeaderboard = true;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    /** ÊâìÂºÄ SC Ë°®Âçï */\r\n    toggleSCForm() {\r\n      if(this.showSCForm == false) {\r\n        this.showSCForm = true;\r\n      } else {\r\n        this.showSCForm = false;\r\n      }\r\n      // if (this.sendAsSC) {\r\n      //   this.showSCForm = true;\r\n      // } else {\r\n      //   this.closeSCForm();\r\n      // }\r\n    },\r\n\r\n    /** ÂÖ≥Èó≠ SC Ë°®Âçï */\r\n    closeSCForm() {\r\n      this.showSCForm = false;\r\n      this.sendAsSC = false;\r\n    },\r\n\r\n    /** ÈÄâÊã©ÈáëÈ¢ù */\r\n    selectSCAmount(amount) {\r\n      this.selectedSCAmount = amount;\r\n      this.customAmount = \"\";\r\n    },\r\n\r\n    /** Ëá™ÂÆö‰πâÈáëÈ¢ùËæìÂÖ• */\r\n    onCustomAmountInput() {\r\n      const amount = parseInt(this.customAmount);\r\n      if (!isNaN(amount) && amount >= 30) {\r\n        this.selectedSCAmount = amount;\r\n      }\r\n    },\r\n\r\n    /** Á°ÆËÆ§ÂèëÈÄÅ SC */\r\n    confirmSendSC() {\r\n      const text = this.input.trim();\r\n      if (!text) return;\r\n\r\n      const msg = {\r\n        username: this.currentUser.username,\r\n        content: text,\r\n        type: \"sc\",\r\n        color: \"#ff9f9f\",\r\n        scAmount: this.selectedSCAmount,\r\n        id: Date.now(),\r\n      };\r\n\r\n      // ÂèëÈÄÅÁªôÁà∂ÁªÑ‰ª∂\r\n      this.$emit(\"send-sc\", msg);\r\n\r\n      // Ê∑ªÂä†Âà∞Âõ∫ÂÆöÂ±ïÁ§∫Âå∫\r\n      this.addPinnedSC(msg);\r\n\r\n      // ÈáçÁΩÆ\r\n      this.input = \"\";\r\n      this.closeSCForm();\r\n    },\r\n\r\n    /** ÂèëÈÄÅÊ∂àÊÅØ */\r\n    onSend() {\r\n      const text = this.input.trim();\r\n      if (!text) return;\r\n\r\n      // Â¶ÇÊûúÂãæÈÄâ‰∫Ü SCÔºåÂàôÊâìÂºÄË°®Âçï\r\n      if (this.sendAsSC) {\r\n        this.showSCForm = true;\r\n        return;\r\n      }\r\n\r\n      // ÊôÆÈÄöÂºπÂπï\r\n      this.$emit(\"send-message\", {\r\n        username: this.currentUser.username,\r\n        content: text,\r\n        type: \"text\",\r\n        color: \"#fff\",\r\n      });\r\n\r\n      this.input = \"\";\r\n    },\r\n\r\n    /** Âõ∫ÂÆö SC ÂºπÂπïÂ±ïÁ§∫ 10 Áßí */\r\n    addPinnedSC(msg) {\r\n      this.scPinned.push(msg);\r\n\r\n      setTimeout(() => {\r\n        this.scPinned = this.scPinned.filter((m) => m.id !== msg.id);\r\n      }, 10000);\r\n    },\r\n\r\n    avatarInitial(name) {\r\n      return name ? name.slice(0, 1).toUpperCase() : \"?\";\r\n    },\r\n    avatarColor(name) {\r\n      const str = name || \"u\";\r\n      let h = 0;\r\n      for (let i = 0; i < str.length; i++) h = (h << 5) - h + str.charCodeAt(i);\r\n      return `hsl(${Math.abs(h) % 360} 70% 55%)`;\r\n    },\r\n\r\n    bubbleStyle(msg) {\r\n      if (msg.type === \"sc\") {\r\n        return {\r\n          background:\r\n            \"linear-gradient(90deg, rgba(255,123,123,0.12), rgba(255,187,123,0.08))\",\r\n          border: \"1px solid rgba(255,123,123,0.25)\",\r\n        };\r\n      }\r\n      if (msg.type === \"gift\") {\r\n        return {\r\n          background: \"rgba(255,209,102,0.06)\",\r\n          border: \"1px solid rgba(255,209,102,0.14)\",\r\n        };\r\n      }\r\n      return {\r\n        background: \"rgba(255,255,255,0.03)\",\r\n        border: \"1px solid rgba(255,255,255,0.03)\",\r\n      };\r\n    },\r\n\r\n    scrollToBottom() {\r\n      const el = this.$refs.messagesList;\r\n      if (el) el.scrollTop = el.scrollHeight;\r\n    },\r\n\r\n    onMouseMove(e) {\r\n      const rect = this.$refs.chatContainer.getBoundingClientRect();\r\n      const y = e.clientY - rect.top;\r\n      if (y <= 60) {\r\n        this.showLeaderboard = true;\r\n      } else if (!this.keepLeaderboard) {\r\n        this.showLeaderboard = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.live-chat {\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background: #fcf5ff;\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n  position: relative;\r\n  border: 1px solid #e5e7eb;\r\n}\r\n\r\n/* Âõ∫ÂÆö SC ÊòæÁ§∫Âå∫ */\r\n.sc-pinned {\r\n  padding: 8px 12px;\r\n  border-bottom: 1px solid #e5e7eb;\r\n  background: #fff9f9;\r\n}\r\n.sc-item {\r\n  padding: 8px 10px;\r\n  margin-bottom: 6px;\r\n  border-radius: 10px;\r\n  font-size: 14px;\r\n  border: 1px solid rgba(255, 123, 123, 0.25);\r\n}\r\n.sc-item .username {\r\n  font-weight: 700;\r\n  color: #222;\r\n}\r\n.sc-item .sc-amount {\r\n  margin-top: 4px;\r\n  color: #e55;\r\n  font-weight: bold;\r\n}\r\n\r\n.room-title {\r\n  font-weight: 600;\r\n  color: #222;\r\n}\r\n\r\n/* Á§ºÁâ©Ê¶ú */\r\n.gift-leaderboard {\r\n  position: absolute;\r\n  left: 12px;\r\n  top: 56px;\r\n  width: 220px;\r\n  background: white;\r\n  border-radius: 10px;\r\n  padding: 10px;\r\n  border: 1px solid #e5e7eb;\r\n  box-shadow: 0 8px 20px rgba(0,0,0,0.06);\r\n  z-index: 20;\r\n}\r\n\r\n\r\n/* ÂçïÊù°Ê∂àÊÅØ */\r\n.msg {\r\n  display: flex;\r\n  gap: 8px;\r\n}\r\n\r\n/* SC ÂãæÈÄâÊåâÈíÆÂå∫Âüü */\r\n.sc-toggle {\r\n  padding: 6px 12px;\r\n  background: #fefbff;\r\n  border-top: 1px solid #e5e7eb;\r\n  font-size: 14px;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 6px;\r\n}\r\n\r\n\r\n.send-box input {\r\n  flex: 1;\r\n  height